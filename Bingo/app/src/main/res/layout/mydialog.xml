<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="290dp"
    android:minHeight="290dp"
    android:background="@drawable/aboutus"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.snicket.premsourav.bingo.MainActivity">

    <TextView
        android:text="\n\n\n\n\n SNICKET INC."
        android:gravity="center"
        android:textColor="#ff3f5a"
        android:textSize="25dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv1"
        android:layout_alignParentStart="true" />
    <TextView
        android:text="\n\nDevelopers:\n\nPREM SOURAV\nNIKET MISHRA\n\nsnicketinc@gmail.com"
        android:gravity="center"
        android:textColor="#ffffff"
        android:textSize="18dp"
        android:fontFamily="casual"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tv2"
        android:layout_below="@+id/tv1"
        android:layout_alignParentStart="true" />

    <Button
        android:text="BACK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv2"
        android:textColor="#ffffff"
        android:background="#ff3f5a"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:id="@+id/b1" />

</RelativeLayout>